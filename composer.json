{
    "name": "beheh/flaps",
    "type": "library",
    "description": "Lightweight library for rate limiting requests",
    "keywords": ["rate limit", "throttle", "HTTP", "429 Too Many Requests"],
	"homepage": "https://github.com/beheh/flaps",
    "license": "ISC",
    "authors": [
        {
            "name": "Benedict Etzel",
            "email": "developer@beheh.de",
            "homepage": "https://beheh.de",
			"role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
		"doctrine/cache": "~1.4",
		"predis/predis": "~1.0"
    },
	"suggest": {
		"predis/predis": "Enable Redis as storage system",
        "doctrine/cache": "Enables a wide variety of caching systems as storage"
    },
    "autoload": {
        "psr-4": {
            "BehEh\\Flaps\\": "src/"
        }
    }	
}
